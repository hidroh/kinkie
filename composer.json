{
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/Barbarrosa/Ratchet",
			"reference": "49c39485e8aa9ed64edc4d64bcb9f1d74bd0ef1e"
		}
	],
	"require": {
		"laravel/framework": "4.0.*",
		"cboden/Ratchet": "dev-patch-1"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		]
	},
	"scripts": {
		"pre-update-cmd": [
			"php artisan clear-compiled"
		],
		"post-install-cmd": [
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan optimize"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
